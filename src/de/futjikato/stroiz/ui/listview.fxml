                     <VBox.margin>
                        <Insets right="10.0" />
                     </VBox.margin></ComboBox>
               </children>
            </VBox>
            <TreeView fx:id="clientTree" nodeOrientation="LEFT_TO_RIGHT" prefHeight="379.0" prefWidth="407.0" />
         </children>
      </HBox>
   </children>
